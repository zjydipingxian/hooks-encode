{
  "name": "hooks-encode",
  "version": "1.0.0",
  "description": "前端vue3 hooks 编码",
  "private": true,
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "all": "pnpm install",
    "clean-dist": "pnpm -r --filter=./packages/* run clearDist",
    "clean": "pnpm -r --filter=./packages/* run clean && rimraf node_modules",
    "build": "pnpm -r --filter=./packages/* run build",
    "pub-before": "tsx scripts/pub-before.ts",
    "pub": "pnpm run clean-dist &&  pnpm run build && pnpm -r --filter=./packages/*  publish --access=public",
    "pub:beta": "pnpm run build && pnpm -r --filter=./packages/* publish --tag beta",
    "format": "prettier --write ./packages/**/*.{js,ts}  --fix",
    "eslint": "eslint  --fix  ./packages/**/*.{js,ts}",
    "cli": "tsx scripts/commander.ts create",
    "wt": "tsx cli/watch.ts",
    "docs:dev": "tsx cli/start.ts",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "prepare": "husky install",
    "commit": "git-cz",
    "commitlint": "commitlint --edit",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "changeset": "changeset",
    "packages-version": "changeset version",
    "test": "jest"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "keywords": [
    "encode",
    "hooks",
    "hook",
    "vue3",
    "vue",
    "Vue",
    "Vue3"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@babel/generator": "^7.25.0",
    "@babel/parser": "^7.25.0",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.24.8",
    "@babel/preset-typescript": "^7.24.7",
    "@babel/traverse": "^7.25.1",
    "@babel/types": "^7.25.0",
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.27.7",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.7.0",
    "@inquirer/prompts": "^5.3.2",
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@testing-library/jest-dom": "^6.4.8",
    "@types/node": "^20.14.11",
    "@vitepress-demo-preview/component": "^2.3.2",
    "@vitepress-demo-preview/plugin": "^1.2.3",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/core": "^11.1.0",
    "babel-jest": "^29.7.0",
    "child_process": "^1.0.2",
    "chokidar": "^3.6.0",
    "commander": "^12.1.0",
    "commitizen": "^4.3.0",
    "conventional-changelog-cli": "^5.0.0",
    "cspell": "^8.10.4",
    "cz-git": "^1.9.4",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "execa": "6.1.0",
    "fs-extra": "^11.2.0",
    "globals": "^15.8.0",
    "husky": "8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "json-templater": "^1.2.0",
    "nodemon": "^3.1.4",
    "ora": "^8.0.1",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.18.1",
    "rollup-plugin-dts": "^6.1.1",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.3",
    "tsx": "^4.17.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.1",
    "vitepress": "^1.2.3",
    "vue": "^3.4.38"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "ant-design-vue": "4.x"
  }
}
